{
  "name": "new-front",
  "license": "GNU AGPLv3",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8002",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint -fix"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "buefy": "^0.8.6",
    "bulma": "^0.8.0",
    "core-js": "^3.3.2",
    "dayjs": "^1.8.16",
    "idb": "^4.0.5",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-infinite-loading": "^2.4.4",
    "vue-offline": "^2.0.8",
    "vue-router": "^3.1.3",
    "vue-scrollto": "^2.17.1",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-pwa": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-buefy": "^0.3.7",
    "vue-template-compiler": "^2.6.10"
  },
  "vue": {
    "pwa": {
      "assetsVersion": "1",
      "workboxOptions": {
        "skipWaiting": true
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "import/prefer-default-export": "off",
      "func-names": "off",
      "no-restricted-syntax": "off",
      "max-len": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
